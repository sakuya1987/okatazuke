	<div class="form-structor">
	 
  
	<div class="signup">
		<div class="center">
			<h2 class="form-title" id="login"><span>or</span>Log in</h2>
			<div class="form-holder">

 <%= form_with model: @user, url: user_session_path, id: 'new_user', class: 'new_user', local: true do |f| %>
   <%= render "devise/shared/error_messages", resource: resource %>

  <div class="input">
    <%#%= f.label :email %>
    <%= f.email_field :email, autofocus: true, class:"input", id:"email", placeholder:"メールアドレス", autocomplete: "email" %>
  </div>

  <div class="input">
    <%#%= f.label :password %>
     <%= f.password_field :password, class:"input", placeholder:"パスワード", id:"password",autocomplete: "current-password" %>
  </div>

  <div class="button">
    <%= f.submit "Log in" ,class:"submit-btn"%>
  </div>
<% end %>

	</div>
	</div>
</div>


<%#%= render "devise/shared/links" %>
